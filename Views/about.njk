{% extends "layout.njk" %}

{% block sobre %}
<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="/sobre.css">
    <title>Sobre - Rocketseat</title>
</head>
<body>
    <header>
        <div class="header">
            <img src="../Assets/logo-rocketseat.svg">
        </div>
    </header>
    
    <h2>Principais tecnologias</h2>
    <div class="grid">
        <div class="card">
            <img src="../Assets/node.png" alt="Node">
        </div>
        <div class="card">
            <img src="../Assets/react.png" alt="React-JS">
        </div>
        <div class="card">
            <img src="../Assets/native.png" alt="React-Native">
        </div>
    </div>
    <footer>
        {% for link in sobre.link%}
        <a href="{{link.url }}" target="_blank">{{link.github}}</a>
        {% endfor %}
    </footer>
</body>
</html>
{% endblock %}